{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:default",
    "log:default",
    "dialog:default",
    "http:default",
    "notification:default",
    "os:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-load",
    "store:default",
    "shell:allow-execute",
    "shell:allow-open",
    "shell:allow-spawn",
    "shell:allow-stdin-write",
    "shell:default"
  ]
}
